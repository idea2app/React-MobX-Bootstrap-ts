{
    "extends": "@parcel/config-default",
    "transformers": {
        "*.{css,less}": [
            "@parcel/transformer-postcss"
        ],
        "*.webmanifest": [
            "@parcel/transformer-webmanifest"
        ],
        "*.{ts,tsx}": [
            "@parcel/transformer-typescript-tsc"
        ],
        "*.{png}": [
            "@parcel/transformer-raw"
        ]
    }
}
